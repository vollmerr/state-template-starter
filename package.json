{
  "name": "state-template-starter",
  "description": "Starter kit for CA State template styled React projects using the state-template package.",
  "author": "California Department of Technology",
  "version": "0.1.3",
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./scripts/build production",
    "build:staging": "node ./scripts/build staging",
    "test": "react-scripts test --coverage --watchAll=false --transformIgnorePatterns \"node_modules/(?!(state-template))/\"",
    "test:watch": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(state-template))/\"",
    "analyze": "source-map-explorer ./build/static/js/*.js",
    "lint": "eslint src/**/*.js"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.2.6",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "state-template": "^0.4.10"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "env-cmd": "^10.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-airbnb": "^18.0.1",
    "mockdate": "^2.0.5",
    "react-scripts": "^3.2.0",
    "source-map-explorer": "^1.8.0"
  },
  "jest": {
    "resetMocks": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
